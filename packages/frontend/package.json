{
  "name": "@cicada/frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:nonprod": "vite --mode nonprod",
    "build": "tsc && vite build",
    "build:nonprod": "tsc && vite build --mode nonprod",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "preview:nonprod": "vite preview --mode nonprod",
    "preview:prod": "vite preview --mode production",
    "test": "echo 'No unit tests configured for frontend. E2E tests would use Playwright.'",
    "test:e2e": "echo 'E2E tests not yet configured'",
    "lint": "eslint . --ext ts,tsx"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.13.1",
    "@cicada/shared-types": "workspace:*",
    "aws-amplify": "^6.15.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
