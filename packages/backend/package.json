{
  "name": "@cicada/backend",
  "version": "1.0.0",
  "description": "CICADA backend Lambda functions and services",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:nonprod": "tsc",
    "build:prod": "tsc",
    "test": "jest --runInBand",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:property": "jest --testPathPattern=test/property",
    "migrate": "ts-node src/scripts/run-migration.ts",
    "eval": "ts-node src/evaluation/scripts/run-eval.ts",
    "eval:citations": "ts-node src/evaluation/scripts/run-eval.ts citation-accuracy.json",
    "eval:theory": "ts-node src/evaluation/scripts/run-eval.ts theory-coherence.json",
    "eval:story": "ts-node src/evaluation/scripts/run-eval.ts story-coherence.json",
    "eval:general": "ts-node src/evaluation/scripts/run-eval.ts general-correctness.json",
    "perf": "ts-node src/scripts/performance-test.ts",
    "optimize": "ts-node src/scripts/optimize-agent-instructions.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.943.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.943.0",
    "@aws-sdk/client-bedrock-runtime": "^3.943.0",
    "@aws-sdk/client-dynamodb": "^3.943.0",
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/lib-dynamodb": "^3.943.0",
    "@cicada/shared-types": "workspace:*",
    "@strands-agents/sdk": "^0.1.2",
    "aws-jwt-verify": "^4.0.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.943.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.943.0",
    "@aws-sdk/client-sfn": "^3.943.0",
    "@aws-sdk/client-sqs": "^3.943.0",
    "@aws-sdk/util-dynamodb": "^3.943.0",
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "fast-check": "^3.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
