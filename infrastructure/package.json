{
  "name": "@cicada/infrastructure",
  "version": "1.0.0",
  "bin": {
    "infrastructure": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "dotenv -e ../.env.nonprod -- cdk synth --profile cicada-deployer",
    "synth:prod": "dotenv -e ../.env.prod -- cdk synth --profile cicada-deployer",
    "deploy": "dotenv -e ../.env.nonprod -- cdk deploy --all --profile cicada-deployer",
    "deploy:prod": "dotenv -e ../.env.prod -- cdk deploy --all --profile cicada-deployer",
    "deploy:data": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADADataStack --profile cicada-deployer",
    "deploy:agent": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADAAgentStack --profile cicada-deployer",
    "deploy:auth": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADAAuthStack --profile cicada-deployer",
    "deploy:api": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADAAPIStack --profile cicada-deployer",
    "deploy:frontend": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADAFrontendStack --profile cicada-deployer",
    "deploy:monitoring": "dotenv -e ../.env.nonprod -- cdk deploy ProjectCICADAMonitoringStack --profile cicada-deployer",
    "diff": "dotenv -e ../.env.nonprod -- cdk diff --profile cicada-deployer",
    "list": "cdk list --profile cicada-deployer",
    "destroy": "dotenv -e ../.env.nonprod -- cdk destroy --all --profile cicada-deployer"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.943.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.25",
    "aws-cdk": "^2.100.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
